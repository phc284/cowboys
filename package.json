{
  "name": "cowboys",
  "version": "1.0.0",
  "description": "Website with dallas cowboys resources",
  "main": "index.js",
  "homepage": "http://phc284.github.io/cowboys",
  "scripts": {
    "predeploy": "cd client && npm run build",
    "deploy": "cd client && gh-pages -d build",
    "build": "cd client && npm install --only=dev && npm install && npm run build",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "concurrently \"cd server && nodemon index.js\" \"cd client && npm start\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build",
  },
  "author": "Peter Choi",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.2",
    "gh-pages": "^1.1.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
